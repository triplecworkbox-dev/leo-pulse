        // Generate star rating HTML
        function generateStars(rating) {
            const fullStars = Math.floor(rating);
            const halfStar = rating % 1 >= 0.5;
            let stars = '';
            
            for (let i = 0; i < fullStars; i++) {
                stars += '‚òÖ';
            }
            if (halfStar) {
                stars += '‚Ø®';
            }
            const emptyStars = 5 - Math.ceil(rating);
            for (let i = 0; i < emptyStars; i++) {
                stars += '‚òÜ';
            }
            
            return stars;
        }

        // Get rating class
        function getRatingClass(rating) {
            if (rating >= 4.5) return 'rating-excellent';
            if (rating >= 4) return 'rating-good';
            if (rating >= 3) return 'rating-average';
            return 'rating-poor';
        }

        // Generate player cards
        function generatePlayerCards(playerList) {
            const cardView = document.getElementById('cardView');
            cardView.innerHTML = '';

            playerList.forEach(player => {
                const card = document.createElement('div');
                card.className = 'player-card';
                
                let statsHTML = '';
                if (player.role === 'bowler') {
                    statsHTML = `
                        <div class="stat-item">
                            <label>Wickets</label>
                            <value>${player.wickets || 'N/A'}</value>
                        </div>
                        <div class="stat-item">
                            <label>Economy</label>
                            <value>${player.economy || 'N/A'}</value>
                        </div>
                        <div class="stat-item">
                            <label>Bowling Avg</label>
                            <value>${player.bowlingAvg || 'N/A'}</value>
                        </div>
                        <div class="stat-item">
                            <label>Dot Ball %</label>
                            <value>${player.dotBallsBowled}%</value>
                        </div>
                    `;
                } else if (player.role === 'all-rounder') {
                    statsHTML = `
                        <div class="stat-item">
                            <label>Batting Avg</label>
                            <value>${player.battingAvg}</value>
                        </div>
                        <div class="stat-item">
                            <label>Strike Rate</label>
                            <value>${player.strikeRate}</value>
                        </div>
                        <div class="stat-item">
                            <label>Wickets</label>
                            <value>${player.wickets || 'N/A'}</value>
                        </div>
                        <div class="stat-item">
                            <label>Economy</label>
                            <value>${player.economy || 'N/A'}</value>
                        </div>
                    `;
                } else {
                    statsHTML = `
                        <div class="stat-item">
                            <label>Batting Avg</label>
                            <value>${player.battingAvg}</value>
                        </div>
                        <div class="stat-item">
                            <label>Strike Rate</label>
                            <value>${player.strikeRate}</value>
                        </div>
                        <div class="stat-item">
                            <label>Runs</label>
                            <value>${player.runs}</value>
                        </div>
                        <div class="stat-item">
                            <label>Boundary %</label>
                            <value>${player.boundaryPct}%</value>
                        </div>
                    `;
                }

                card.innerHTML = `
                    <div class="player-header">
                        <div class="player-info">
                            <h3>${player.name}</h3>
                            <p>${player.age} years ‚Ä¢ ${player.role.replace('-', ' ').replace(/\b\w/g, l => l.toUpperCase())}</p>
                            <p style="color: #006400; font-weight: 600;">${player.team}</p>
                        </div>
                        <div class="overall-rating">
                            <span class="score ${getRatingClass(player.overall)}">${player.overall.toFixed(1)}</span>
                            <span class="label">Overall</span>
                        </div>
                    </div>
                    <div class="stats-grid">
                        ${statsHTML}
                    </div>
                    <div class="ratings-row">
                        <div class="rating-item">
                            <label>Performance</label>
                            <div class="stars">${generateStars(player.performance)}</div>
                        </div>
                        <div class="rating-item">
                            <label>Consistency</label>
                            <div class="stars">${generateStars(player.consistency)}</div>
                        </div>
                        <div class="rating-item">
                            <label>Fitness</label>
                            <div class="stars">${generateStars(player.fitness)}</div>
                        </div>
                    </div>
                    <div class="player-actions">
                        <button class="btn btn-primary" onclick="sendInvitation('${player.name}', '${player.email}')">
                            üìß Send Invitation
                        </button>
                        <button class="btn btn-secondary" onclick="viewProfile('${player.name}')">
                            üë§ View Profile
                        </button>
                    </div>
                `;
                
                cardView.appendChild(card);
            });
        }

        // Generate table rows
        function generateTableRows(playerList) {
            const tableBody = document.getElementById('tableBody');
            tableBody.innerHTML = '';

            playerList.forEach(player => {
                const row = document.createElement('tr');
                
                let keyStats = '';
                if (player.role === 'bowler') {
                    keyStats = `Wkts: ${player.wickets || 'N/A'}, Econ: ${player.economy || 'N/A'}`;
                } else if (player.role === 'all-rounder') {
                    if (player.runs && player.wickets) {
                        keyStats = `Runs: ${player.runs}, Wkts: ${player.wickets}`;
                    } else {
                        keyStats = `Avg: ${player.battingAvg}, SR: ${player.strikeRate}`;
                    }
                } else {
                    keyStats = `Avg: ${player.battingAvg}, SR: ${player.strikeRate}`;
                }

                row.innerHTML = `
                    <td><strong>${player.name}</strong><br><small style="color: #666;">${player.team}</small></td>
                    <td>${player.role.replace('-', ' ').replace(/\b\w/g, l => l.toUpperCase())}</td>
                    <td>${player.age}</td>
                    <td><strong class="${getRatingClass(player.overall)}">${player.overall.toFixed(1)}</strong></td>
                    <td><span class="stars">${generateStars(player.performance)}</span></td>
                    <td><span class="stars">${generateStars(player.consistency)}</span></td>
                    <td><span class="stars">${generateStars(player.fitness)}</span></td>
                    <td>${keyStats}</td>
                    <td>
                        <button class="btn btn-primary" onclick="sendInvitation('${player.name}', '${player.email}')">Invite</button>
                    </td>
                `;
                
                tableBody.appendChild(row);
            });
        }

        // Filter players
        function filterPlayers() {
            const roleFilter = document.getElementById('roleFilter').value;
            const regionFilter = document.getElementById('regionFilter').value;
            const ageFilter = document.getElementById('ageFilter').value;
            const searchInput = document.getElementById('searchInput').value.toLowerCase();

            let filtered = players.filter(player => {
                const roleMatch = roleFilter === 'all' || player.role === roleFilter;
                
                // Team filter matching
                let regionMatch = true;
                if (regionFilter !== 'all') {
                    const teamSlug = player.team.toLowerCase().replace(/\s+/g, '-');
                    regionMatch = teamSlug === regionFilter;
                }
                
                let ageMatch = true;
                if (ageFilter === 'u20') ageMatch = player.age < 20;
                else if (ageFilter === 'u23') ageMatch = player.age >= 20 && player.age <= 22;
                else if (ageFilter === 'u26') ageMatch = player.age >= 23 && player.age <= 25;
                else if (ageFilter === 'senior') ageMatch = player.age >= 26;

                const searchMatch = player.name.toLowerCase().includes(searchInput) || 
                                  player.team.toLowerCase().includes(searchInput);

                return roleMatch && regionMatch && ageMatch && searchMatch;
            });

            // Sort by overall rating
            filtered.sort((a, b) => b.overall - a.overall);

            generatePlayerCards(filtered);
            generateTableRows(filtered);
        }

        // Toggle view
        function toggleView(view) {
            const cardView = document.getElementById('cardView');
            const tableView = document.getElementById('tableView');
            const buttons = document.querySelectorAll('.view-btn');

            buttons.forEach(btn => btn.classList.remove('active'));

            if (view === 'card') {
                cardView.style.display = 'grid';
                tableView.classList.remove('active');
                buttons[0].classList.add('active');
            } else {
                cardView.style.display = 'none';
                tableView.classList.add('active');
                buttons[1].classList.add('active');
            }
        }

        // Send invitation
        function sendInvitation(name, email) {
            alert(`üìß Invitation sent to ${name} (${email})!\n\nThe player has been notified about the opportunity to join the Boland Cricket talent pipeline.`);
        }

        // View profile
        function viewProfile(name) {
            alert(`üë§ Opening detailed profile for ${name}...\n\nThis would show:\n‚Ä¢ Complete match history\n‚Ä¢ Performance trends over T10 Season 2\n‚Ä¢ Video highlights\n‚Ä¢ Scout reports\n‚Ä¢ AI predictions\n‚Ä¢ Venue-specific analytics`);
        }

        // Show subscription modal
        function showSubscription() {
            document.getElementById('subscriptionModal').classList.add('active');
        }

        // Close subscription modal
        function closeSubscription() {
            document.getElementById('subscriptionModal').classList.remove('active');
        }

        // Initialize
        document.addEventListener('DOMContentLoaded', function() {
            filterPlayers();
        });
    </script>
